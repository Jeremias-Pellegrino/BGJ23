;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "CountdownTrigger",
"language": "GDScript",
"path": "res://Scripts/CountdownTrigger.gd"
}, {
"base": "Light2D",
"class": "Lantern",
"language": "GDScript",
"path": "res://Scripts/Lantern.gd"
}, {
"base": "Light2D",
"class": "MenuLight",
"language": "GDScript",
"path": "res://Scripts/MenuLight.gd"
}, {
"base": "Light2D",
"class": "SineLight",
